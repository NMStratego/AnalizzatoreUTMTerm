{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "excludeFiles": "{uploads/**,__pycache__/**,*.pyc,*.pyo,*.pyd,.git/**,.gitignore,README.md,*.md,static/**}"
    }
  }
}